{
  "name": "nert-restoration-tracker-app",
  "version": "0.0.0",
  "description": "NERT Restoration Tracker Web App",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/restoration-tracker.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy_start": "node server",
    "test": "react-scripts test --env=jsdom-fourteen --ci --watchAll=false --runInBand",
    "test-watch": "react-scripts test --env=jsdom-fourteen",
    "coverage": "react-scripts test --ci --coverage --testResultsProcessor jest-sonar-reporter --env=jsdom-fourteen --watchAll=false --runInBand",
    "update-snapshots": "react-scripts test --env=jsdom-fourteen --ci --watchAll=false --updateSnapshot",
    "lint": "eslint src/ --ext .jsx,.js,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\""
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 10.0.0"
  },
  "dependencies": {
    "@bcgov/bc-sans": "~2.1.0",
    "@emotion/styled": "11.11.0",
    "@mdi/js": "~7.4.47",
    "@mdi/react": "~1.6.1",
    "@mui/icons-material": "~5.15.10",
    "@mui/lab": "~5.0.0-alpha.165",
    "@mui/material": "~5.15.10",
    "@mui/styled-engine": "5.15.11",
    "@mui/styles": "~5.15.10",
    "@mui/system": "~5.15.9",
    "@react-keycloak/web": "^3.4.0",
    "@react-leaflet/core": "~1.1.1",
    "@tmcw/togeojson": "~5.8.1",
    "@turf/bbox": "~6.5.0",
    "@turf/boolean-equal": "~6.5.0",
    "@turf/centroid": "~6.5.0",
    "axios": "^1.6.7",
    "buffer": "^6.0.3",
    "clsx": "~2.1.0",
    "express": "~4.18.2",
    "formik": "~2.4.5",
    "keycloak-js": "~23.0.7",
    "leaflet": "~1.9.4",
    "leaflet-draw": "~1.0.4",
    "leaflet-fullscreen": "~1.0.2",
    "leaflet.locatecontrol": "^0.76.1",
    "lodash-es": "~4.17.21",
    "moment": "~2.30.1",
    "qs": "~6.11.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-dropzone": "~14.2.3",
    "react-leaflet": "~3.2.5",
    "react-leaflet-cluster": "~1.0.4",
    "react-number-format": "~5.3.3",
    "react-router": "~5.3.4",
    "react-router-dom": "~5.3.4",
    "react-window": "~1.8.10",
    "reproj-helper": "~1.3.1",
    "shpjs": "^4.0.4",
    "typescript": "~4.1.6",
    "uuid": "~9.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "~7.21.11",
    "@babel/preset-typescript": "~7.23.3",
    "@testing-library/jest-dom": "~6.4.2",
    "@testing-library/react": "~12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "~14.5.2",
    "@types/geojson": "~7946.0.14",
    "@types/jest": "~29.5.12",
    "@types/leaflet": "^1.9.8",
    "@types/leaflet-draw": "~1.0.11",
    "@types/leaflet-fullscreen": "~1.0.9",
    "@types/lodash-es": "~4.17.12",
    "@types/node": "~20.11.20",
    "@types/qs": "~6.9.11",
    "@types/react": "~17.0.39",
    "@types/react-dom": "~17.0.25",
    "@types/react-leaflet": "~3.0.0",
    "@types/react-router": "~5.1.20",
    "@types/react-router-dom": "~5.3.3",
    "@types/react-window": "~1.8.8",
    "@types/shpjs": "^3.4.6",
    "@types/uuid": "~9.0.8",
    "@typescript-eslint/eslint-plugin": "~7.0.2",
    "@typescript-eslint/parser": "~7.0.2",
    "axios-mock-adapter": "~1.22.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-prettier": "~3.3.1",
    "jest": "~29.7.0",
    "jest-sonar-reporter": "~2.0.0",
    "prettier": "~2.8.8",
    "prettier-plugin-organize-imports": "~3.2.4",
    "react-scripts": "~5.0.1",
    "sass": "^1.71.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!lodash-es)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/themes/**",
      "!<rootDir>/src/constants/**",
      "!<rootDir>/src/App.tsx",
      "!<rootDir>/src/types/**",
      "!<rootDir>/src/interfaces/**",
      "!<rootDir>/src/AppRouter.tsx",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/node_modules/**",
      "!<rootDir>/coverage/**",
      "!<rootDir>/public/**",
      "!<rootDir>/build/**",
      "!<rootDir>/src/serviceWorker.**",
      "!<rootDir>/src/setupTests.*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
