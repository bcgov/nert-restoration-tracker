{
  "name": "nert-restoration-tracker-app",
  "version": "0.0.0",
  "description": "NERT Restoration Tracker Web App",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/nert-restoration-tracker.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy_start": "node --max-old-space-size=2000 server",
    "test": "react-scripts test --ci --watchAll=false --runInBand",
    "test-watch": "react-scripts test",
    "coverage": "react-scripts test --ci --coverage --testResultsProcessor jest-sonar-reporter --watchAll=false --runInBand",
    "update-snapshots": "react-scripts test --ci --watchAll=false --updateSnapshot",
    "lint": "eslint src/ --ext .jsx,.js,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\""
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 10.0.0"
  },
  "dependencies": {
    "@bcgov/bc-sans": "~2.1.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mdi/js": "~7.4.47",
    "@mdi/react": "~1.6.1",
    "@mui/icons-material": "~5.15.12",
    "@mui/lab": "~5.0.0-alpha.167",
    "@mui/material": "~5.15.10",
    "@mui/styled-engine": "5.15.11",
    "@mui/system": "~5.15.12",
    "@mui/x-date-pickers": "^7.11.0",
    "@react-keycloak/web": "^3.4.0",
    "@turf/bbox": "~6.5.0",
    "@turf/boolean-equal": "~6.5.0",
    "@turf/centroid": "~6.5.0",
    "@turf/turf": "^6.5.0",
    "axios": "^1.7.2",
    "buffer": "^6.0.3",
    "clsx": "~2.1.0",
    "dayjs": "^1.11.12",
    "express": "~4.18.3",
    "formik": "~2.4.5",
    "keycloak-js": "~23.0.7",
    "lodash-es": "~4.17.21",
    "maplibre-gl": "^4.5.0",
    "oidc-client-ts": "^3.0.1",
    "qs": "~6.11.2",
    "react": "~18.2.0",
    "react-collapsed": "^4.1.2",
    "react-dom": "~18.2.0",
    "react-dropzone": "~14.2.3",
    "react-number-format": "~5.3.3",
    "react-oidc-context": "^3.1.0",
    "react-player": "^2.16.0",
    "react-router": "~6.22.3",
    "react-router-dom": "~6.22.3",
    "react-router-prompt": "^0.7.0",
    "react-window": "~1.8.10",
    "reproj-helper": "~1.3.1",
    "request": "^2.88.2",
    "typescript": "~4.7.4",
    "uuid": "~9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "~7.21.11",
    "@babel/preset-typescript": "~7.23.3",
    "@testing-library/jest-dom": "~6.4.2",
    "@testing-library/react": "~14.2.1",
    "@testing-library/user-event": "~14.5.2",
    "@types/geojson": "~7946.0.14",
    "@types/jest": "~29.5.12",
    "@types/lodash-es": "~4.17.12",
    "@types/node": "~20.11.20",
    "@types/qs": "~6.9.11",
    "@types/react": "~18.2.64",
    "@types/react-dom": "~18.2.21",
    "@types/react-router": "~5.1.20",
    "@types/react-router-dom": "~5.3.3",
    "@types/react-window": "~1.8.8",
    "@types/uuid": "~9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axios-mock-adapter": "~1.22.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sonar-reporter": "~2.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react-scripts": "~5.0.1",
    "sass": "^1.77.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!lodash-es)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/themes/**",
      "!<rootDir>/src/constants/**",
      "!<rootDir>/src/App.tsx",
      "!<rootDir>/src/types/**",
      "!<rootDir>/src/interfaces/**",
      "!<rootDir>/src/AppRouter.tsx",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/node_modules/**",
      "!<rootDir>/coverage/**",
      "!<rootDir>/public/**",
      "!<rootDir>/build/**",
      "!<rootDir>/src/serviceWorker.**",
      "!<rootDir>/src/setupTests.*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
