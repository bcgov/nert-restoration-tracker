    "gulp-typescript": "6.0.0-alpha.1",
    "mocha": "~10.3.0",
    "nodemon": "~3.0.3",
    "npm-run-all": "~4.1.5",
    "nyc": "~15.1.0",
    "prettier": "~2.8.8",
    "prettier-plugin-organize-imports": "~3.2.4",
    "sinon": "~17.0.1",
    "sinon-chai": "~3.7.0",
    "ts-mocha": "~10.0.0",
    "ts-node": "~10.9.2"
  }
