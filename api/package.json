{
  "name": "nert-restoration-tracker-api",
  "version": "0.0.0",
  "description": "API for NERT Restoration Tracker Web App",
  "license": "Apache-2.0",
  "main": "app",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/nert-restoration-tracker.git"
  },
  "scripts": {
    "start": "ts-node src/app",
    "build": "tsc --project tsconfig.production.json",
    "start-reload": "./node_modules/.bin/nodemon src/app.ts --exec ts-node",
    "test": "mocha",
    "test-watch": "mocha --watch",
    "coverage": "nyc mocha",
    "lint": "eslint . --ignore-pattern 'node_modules' --ext .ts",
    "lint-fix": "eslint . --fix --ignore-pattern 'node_modules' --ext .ts",
    "format": "prettier --loglevel=warn --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-fix": "prettier --loglevel=warn --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "fix": "npm-run-all -l -s lint-fix format-fix"
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 10.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "~8.12.1",
    "@turf/bbox": "~6.5.0",
    "@turf/circle": "~6.5.0",
    "@turf/helpers": "~6.5.0",
    "@turf/meta": "~6.5.0",
    "@turf/turf": "^7.0.0",
    "adm-zip": "^0.5.14",
    "ajv": "~8.12.0",
    "aws-sdk": "~2.1563.0",
    "axios": "~1.6.7",
    "clamdjs": "~1.0.2",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "~1.5.2",
    "express": "~4.18.2",
    "express-openapi": "~11.1.0",
    "fast-json-patch": "~3.1.1",
    "jsonpath": "~1.1.1",
    "jsonwebtoken": "~9.0.2",
    "jwks-rsa": "~3.1.0",
    "knex": "^3.1.0",
    "lodash": "~4.17.21",
    "mime": "~4.0.1",
    "multer": "1.4.5-lts.1",
    "pg": "~8.11.3",
    "qs": "~6.11.2",
    "shpjs": "^4.0.4",
    "sql-template-strings": "~2.2.2",
    "swagger-ui-express": "~5.0.0",
    "typescript": "~5.3.3",
    "uuid": "~9.0.1",
    "winston": "~3.11.0",
    "xlsx": "^0.18.5",
    "xml2js": "~0.6.2",
    "zod": "^3.23.8"
