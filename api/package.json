    "@types/express": "~4.17.21",
    "@types/geojson": "~7946.0.14",
    "@types/gulp": "~4.0.17",
    "@types/jsonpath": "^0.2.4",
    "@types/jsonwebtoken": "~9.0.5",
    "@types/lodash": "~4.14.202",
    "@types/mime": "~3.0.4",
    "@types/mocha": "~10.0.6",
    "@types/multer": "~1.4.11",
    "@types/node": "~20.11.0",
    "@types/pg": "^8.11.6",
    "@types/shpjs": "^3.4.7",
    "@types/sinon": "~17.0.3",
    "@types/sinon-chai": "~3.2.12",
    "@types/swagger-ui-express": "~4.1.6",
    "@types/uuid": "~9.0.8",
    "@types/xml2js": "^0.4.14",
    "@types/yamljs": "^0.2.34",
